{
  "dashboard": {
    "title": "RAG System Overview",
    "tags": ["rag", "overview"],
    "timezone": "browser",
    "schemaVersion": 36,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(http_requests_total, job)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(http_requests_total, tenant_id)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\"$service\",tenant_id=~\"$tenant\"}[5m])) by (job)",
            "legendFormat": "{{ job }}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 2,
        "title": "p95 Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\",tenant_id=~\"$tenant\"}[5m])) by (job, le))",
            "legendFormat": "{{ job }} p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\",tenant_id=~\"$tenant\"}[5m])) by (job, le))",
            "legendFormat": "{{ job }} p99"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "alert": {
          "name": "p95 Latency Alert",
          "message": "p95 latency exceeds 2.1s threshold",
          "conditions": [
            {
              "query": {"queryId": "A", "from": "5m"},
              "reducer": {"type": "last", "queryId": "A"},
              "evaluator": {"params": [2.1], "type": "gt"}
            }
          ]
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\"$service\",status_code=~\"5..\",tenant_id=~\"$tenant\"}[5m])) by (job) / sum(rate(http_requests_total{job=~\"$service\",tenant_id=~\"$tenant\"}[5m])) by (job)",
            "legendFormat": "{{ job }} Error Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Error %",
            "min": 0,
            "max": 1
          }
        ]
      },
      {
        "id": 4,
        "title": "Service Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "up{job=~\"$service\"}",
            "legendFormat": "{{ job }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "Down", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "Up", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Hits"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses"
          }
        ]
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m])))",
            "legendFormat": "Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "L1 Cache Stats",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "rag_cache_l1_size",
            "legendFormat": "Size"
          },
          {
            "expr": "rag_cache_l1_hit_rate",
            "legendFormat": "Hit Rate"
          }
        ]
      }
    ]
  }
}
